        {
          "name": "Search Bands by First Letter",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{token}}" }
            ],
            "url": {
              "raw": "{{baseUrl}}/bands/lists/:firstLetter",
