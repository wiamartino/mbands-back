    {
      "name": "Bands",
      "item": [
        {
          "name": "Create Band",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "Authorization", "value": "Bearer {{token}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Band Name\",\n  \"genre\": \"Rock\",\n  \"yearFormed\": 2000,\n  \"country\": \"USA\",\n  \"active\": true,\n  \"website\": \"http://bandwebsite.com\",\n  \"description\": \"Band description\",\n  \"imageUrl\": \"http://imageurl.com/image.jpg\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/bands",
              "host": ["{{baseUrl}}"],
              "path": ["bands"]
            }
          }
        },
        {
          "name": "Get All Bands",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{token}}" }
            ],
            "url": {
              "raw": "{{baseUrl}}/bands",
              "host": ["{{baseUrl}}"],
              "path": ["bands"],
              "query": [
                { "key": "page", "value": "1" },
                { "key": "limit", "value": "10" }
              ]
            }
          }
        },
        {
          "name": "Get Band by ID",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{token}}" }
            ],
            "url": {
              "raw": "{{baseUrl}}/bands/:id",
              "host": ["{{baseUrl}}"],
              "path": ["bands", ":id"]
            }
          }
        },
        {
          "name": "Update Band",
          "request": {
            "method": "PATCH",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "Authorization", "value": "Bearer {{token}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Band\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/bands/:id",
              "host": ["{{baseUrl}}"],
              "path": ["bands", ":id"]
            }
          }
        },
        {
          "name": "Delete Band",
          "request": {
            "method": "DELETE",
            "header": [
              { "key": "Authorization", "value": "Bearer {{token}}" }
            ],
            "url": {
